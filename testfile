add $s0, $s1, $s2
addi $t0,$s3,-12
addiu $s0, $s2, 42
xor $s7, $a3, $t5
# sdasdasdasdasdasdqweqwe
sll $t0, $t2, 0x1A
mflo $a3 # blabsadfsadfa # asdfsdf
subu $t0, $t1, $t2
slti $k0, $k1, 0xFFFF
sltu $k0, $k1, $t9 # comment
addu $t0, $zero, $zero
and $s0, $5, $31
andi $t9, $t1, 0x33 # QQQQQQQQQQQ
lui $a2, 0x1234
slti $t0, $s0, 16
sltiu $gp, $sp, 256
ori $ra, $31, 1
xori $a2, $a3, 0xABCD
srl $t2, $t3, 0x1F
sra     $t4, $t5, 0x00000
sllv $t6, $t7, $30
srlv $t8, $t9, $s4
srav $s5, $s6, $s7 
mfhi $zero
mthi $at
mflo $v1
mtlo $v0
mult $4, $5
multu $5, $6
div $a2, $a3
divu $6, $7
sub  $8, $9, $10
subu $11, $12, $13 
or $14, $15, $16
xor $17, $18, $19
nor $20, $21, $22
